function toggleImageVideoList( type )
{
	var vidL = $('product-media-videos');
	var picL = $('product-media-images');

	var vidT = $('video_tab');
	var picT = $('image_tab');
	
	switch( type )
	{
		case 'images':
			vidL.hide();
			if (JsObject != "undefined")
			{
				JsObject.hide(); // This stops videos from playing in IE
			}
			picL.show();
			vidT.className = 'last';
			picT.className = 'active';
			break;
		case 'videos':
			if (JsObject != "undefined")
			{
				JsObject.setPrevVideo();
			}
			JsObject.setVideo('816');
			vidL.show();
			picL.hide();
			vidT.className = 'active last';
			picT.className = '';
			break;
	}
}
